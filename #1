#include <iostream>
#include <cstdlib>
#include <string>
using namespace std;

int main()
{
 string s, a = "a", e = "e", y = "y", u = "u", i = "i", o = "o";
 int d = 0, f = 0, g = 0, h = 0, j = 0, k = 0;
 getline(cin,s);
 s.length();
 for (int x = 0; x < s.length(); x++)
	{
 if (s[x] == 'a')
 d++;
 if (s[x] == 'e')
 f++;
 if (s[x] == 'y')
 g++;
 if (s[x] == 'u')
 h++;
 if (s[x] == 'i')
 j++;
 if (s[x] == 'o')
 k++;
 }
    {
        cout << "a = " << d << "; " << "e = " << f << "; " << "y = " << g << "; " << "u = " << h << "; " << "i = " << j << "; " << "o = " << k << "; " << endl;
    }
 return 0;
 }
