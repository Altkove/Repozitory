#include <iostream>
#include <cstdlib>
#include <string>
using namespace std;

int main()
{
 string s, a = "a", e = "e", y = "y", u = "u", i = "i", o = "o";
 int t = 0, c = 0, h = 0, k = 0, m = 0, z = 0;
 getline(cin,s);
 s.length();
 for (int p = 0; p < s.length(); p++)
	{
 if (s[p] == 'a')
 t++;
 if (s[p] == 'e')
 c++;
 if (s[p] == 'y')
 h++;
 if (s[p] == 'u')
 k++;
 if (s[p] == 'i')
 m++;
 if (s[p] == 'o')
 z++;
 }
    {
        cout << "a = " << t << "; " << "e = " << c << "; " << "y = " << h << "; " << "u = " << k << "; " << "i = " << m << "; " << "i = " << z << "; " << endl;
    }
 return 0;
 }
